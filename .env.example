# Research Tool Configuration (OpenRouter Branch)
# Copy to .env and fill in your values

# OpenRouter Configuration (REQUIRED)
RESEARCH_LLM_API_KEY=sk-or-v1-your-openrouter-key-here
RESEARCH_LLM_API_BASE=https://openrouter.ai/api/v1
RESEARCH_LLM_MODEL=alibaba/tongyi-deepresearch-30b-a3b:free
RESEARCH_LLM_MODEL_FALLBACK=alibaba/tongyi-deepresearch-30b-a3b
RESEARCH_LLM_FALLBACK_ENABLED=true

# Optional: Tavily API Key (leave empty to use only SearXNG)
RESEARCH_TAVILY_API_KEY=

# Optional: LinkUp API Key (leave empty to disable)
RESEARCH_LINKUP_API_KEY=

# SearXNG is configured in docker-compose.yml and defaults to 192.168.1.3:8888
